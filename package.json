{
  "name": "browser-automation-benchmark",
  "version": "1.0.0",
  "description": "Comprehensive benchmark suite for browser automation tools: browser-use, dev3000, firecrawl, playwriter, playwright-mcp",
  "type": "module",
  "scripts": {
    "benchmark": "bun run benchmarks/runner.ts",
    "benchmark:scraping": "bun run benchmarks/runner.ts --scenario=scraping",
    "benchmark:navigation": "bun run benchmarks/runner.ts --scenario=navigation",
    "benchmark:debugging": "bun run benchmarks/runner.ts --scenario=debugging",
    "benchmark:forms": "bun run benchmarks/runner.ts --scenario=form-filling",
    "benchmark:search": "bun run benchmarks/runner.ts --scenario=search",
    "benchmark:structured": "bun run benchmarks/runner.ts --scenario=structured-data",
    "results": "bun run benchmarks/utils/show-results.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint benchmarks/"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@mendable/firecrawl-js": "^4.10.0",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "playwright": "^1.57.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.0.0",
    "bun": ">=1.1.0"
  },
  "keywords": [
    "browser-automation",
    "benchmark",
    "playwright",
    "mcp",
    "web-scraping",
    "ai-agents",
    "claude",
    "llm"
  ],
  "license": "MIT"
}
